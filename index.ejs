<ul>
<% for (const item of items) { %>
<li><a href="<%- item.path %>"><%= item.path.substring(item.path.substring(1).indexOf("/")+2, item.path.indexOf(item.filename)) %><% if (item.title) { %>
<b><%= item.title %></b> - <%= item.subtitle %>
<% } else { %>
<b><%= item.filename %></b>
<% } %> </a></li> <% } %>
</ul>
